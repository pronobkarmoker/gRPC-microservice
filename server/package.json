{
  "name": "grpc-user-service",
  "version": "1.0.0",
  "description": "gRPC User Management Service",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "generate-proto": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./generated --grpc_out=grpc_js:./generated --plugin=protoc-gen-grpc=`which grpc_tools_node_protoc_plugin` user_service.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.8",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "grpc-tools": "^1.12.4"
  },
  "keywords": [
    "grpc",
    "microservices",
    "nodejs",
    "protobuf"
  ],
  "author": "Your Name",
  "license": "MIT"
}